<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="es.ujaen.git.sm1718_g06_practica1.MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="onIcon"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="532dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="309dp"
        android:layout_height="95dp"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/uja"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="16dp" />

    <fragment
        android:id="@+id/fragment2"
        android:name="es.ujaen.git.sm1718_g06_practica1.LoginFragment"
        android:layout_width="411dp"
        android:layout_height="420dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView2"
        tools:layout="@layout/fragment_login"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="110dp" />

    <TextView
        android:id="@+id/volatil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name2" />

</android.widget.RelativeLayout>
